{
  "id": "melonbooks",
  "name": "Melonbooks",
  "description": "Search and browse doujinshi from Melonbooks - Japan's leading doujinshi retailer",
  "version": "1.0.0",
  "author": "Tsumihon",
  "authorUrl": null,
  "icon": "storefront",
  "category": "store",
  "baseUrl": "https://www.melonbooks.co.jp",
  "capabilities": {
    "supportsSearch": true,
    "supportsGetById": true,
    "requiresAuth": false,
    "supportsPagination": true,
    "supportsImageSearch": false,
    "supportsIsbnScan": false,
    "requiresConfiguration": false,
    "supportsReading": false,
    "requiresCorsProxy": true
  },
  "apiConfig": {
    "type": "melonbooks_api",
    "endpoints": {
      "search": "/search/search.php",
      "getById": "/detail/detail.php"
    }
  },
  "settings": {
    "type": "none"
  },
  "minAppVersion": "1.0.0"
}
