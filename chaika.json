  "apiConfig": {
    "type": "chaika_api",
    "endpoints": {
      "search": "/api/search",
      "getById": "/api/gallery/{id}"
    }
  },
  "settings": {
    "type": "none"
  },
  "minAppVersion": "1.0.0"
