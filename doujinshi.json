{
  "id": "doujinshi",
  "name": "Doujinshi.info",
  "description": "Comprehensive doujinshi database with multilingual support",
  "version": "1.0.0",
  "author": "Tsumihon",
  "authorUrl": null,
  "icon": "library_books",
  "category": "aggregator",
  "baseUrl": "https://www.doujinshi.info/api",
  "capabilities": {
    "supportsSearch": true,
    "supportsGetById": true,
    "requiresAuth": false,
    "supportsPagination": true,
    "supportsImageSearch": false,
    "requiresConfiguration": false,
    "supportsReading": false
  },
  "apiConfig": {
    "type": "doujinshi_info_api",
    "endpoints": {
      "search": "/search",
      "getById": "/book/{id}"
    }
  },
  "settings": {
    "type": "none"
  },
  "minAppVersion": "1.0.0"
}
